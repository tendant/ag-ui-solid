{
  "name": "@tendant/ag-ui-solid",
  "version": "0.2.5",
  "description": "A SolidJS component library for AI chat interfaces",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/style.css",
  "sideEffects": [
    "./dist/style.css",
    "*.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/style.css"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "link:local": "./scripts/link-local.sh",
    "unlink:local": "./scripts/unlink-local.sh",
    "dev:watch": "./scripts/dev-watch.sh",
    "prepublishOnly": "vitest --run && npm run build",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "peerDependencies": {
    "@kobalte/core": "^0.13.0",
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@kobalte/core": "^0.13.11",
    "@solidjs/testing-library": "^0.8.5",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^23.2.0",
    "lucide-solid": "^0.545.0",
    "postcss": "^8.5.6",
    "solid-js": "^1.8.11",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.8.2",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "solidjs",
    "chat",
    "ai",
    "ui",
    "components"
  ],
  "license": "MIT",
  "dependencies": {
    "@ag-ui/core": "^0.0.39",
    "@solid-primitives/i18n": "^2.2.1",
    "lucide-solid": "^0.545.0",
    "marked": "^16.4.0"
  }
}
